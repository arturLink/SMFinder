<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SMFinder.App">
    <Application.Resources>
        <Style x:Key="NormalFont" TargetType="Label">
            <Setter Property="FontFamily" Value="DancingScript-VariableFont_wght.ttf"/>
        </Style>
        <Style x:Key="BoldFont" TargetType="Label">
            <Setter Property="FontFamily" Value="Pacifico-Regular.ttf"/>
        </Style>
    </Application.Resources>
</Application>